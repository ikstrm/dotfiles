# ls
alias ls="ls -Gla"

# override system ctags
alias ctags="/usr/local/bin/ctags"

# .zshrc reload
alias re="source ~/.zshrc"

# vim
alias vi="/usr/local/bin/vim"
alias vim="/usr/local/bin/vim"

# cd by dir name only for ..
alias ..="cd .."

