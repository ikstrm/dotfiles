{
  "Keymap": {
    "C-c": "peco.Cancel",
    "C-d": "peco.EndOfFile",
    "C-v": "peco.ToggleSelectMode",
    "M-d": "peco.DeleteForwardWord",
    "C-i": {
      "C-f": "peco.SelectNextPage",
      "C-b": "peco.SelectPreviousPage"
    }
  },
  "Style": {
    "Basic": ["on_default", "default"],
    "SavedSelection": ["on_blue", "black"],
    "Selected": ["on_cyan", "black"],
    "Query": ["on_default", "default"],
    "Matched": ["yellow", "bold"]
  }
}
