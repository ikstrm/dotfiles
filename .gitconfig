[user]
    name = Takashi Kokubun
    email = kokubun.t.aa@m.titech.ac.jp
[color]
    ui = true
[core]
    excludesfile = ~/.gitignore
[alias]
    co = checkout
    st = status -sb
    pr = pull --rebase
    fo = fetch origin
    ro = rebase origin
    rc = rebase --continue
    wd = diff --word-diff
    gp = grep -n
    lg = log --graph --pretty=oneline --decorate --date=short --abbrev-commit --branches
